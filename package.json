{
  "name": "browser-threat-sim",
  "version": "1.0.0",
  "description": "AI-driven phishing simulation extension for security training",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "cd dashboard && npm run build",
    "test": "node --test tests/*.test.cjs"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "keywords": [
    "security",
    "phishing",
    "training",
    "ai",
    "chrome-extension"
  ],
  "author": "Security AI Stack",
  "license": "MIT"
}
